{
  "name": "Easy",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package && husky",
    "set-env:local": "run-script-os",
    "set-env:local:windows": "SET ENVFILE=.env.local",
    "set-env:local:nix": "ENVFILE=.env.local",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 14 Pro Max'",
    "lint": "eslint ./src --ext .ts,.tsx --cache",
    "lint-fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "start": "react-native start",
    "start-reset-cache": "yarn start --reset-cache",
    "test": "jest",
    "devtools": "npx react-devtools",
    "pod-install": "cd ios && pod install && cd ..",
    "prepare": "husky",
    "create-appicon": "iconset create _assets/logo.png",
    "generate-splash-screen": "react-native generate-bootsplash _assets/logo.png --platforms=android,ios --background=6871EE --logo-width=140 --assets-output=assets --flavor=main"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.19",
    "@react-navigation/devtools": "^6.0.25",
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/native-stack": "^6.9.25",
    "@reduxjs/toolkit": "^2.2.1",
    "nanoid": "^5.0.6",
    "react": "18.2.0",
    "react-hook-form": "^7.51.0",
    "react-native": "0.73.6",
    "react-native-bootsplash": "^5.4.1",
    "react-native-config": "^1.5.1",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-maps": "^1.12.0",
    "react-native-mmkv-storage": "^0.9.1",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^15.1.0",
    "react-redux": "^9.1.0",
    "redux-flipper": "^2.0.3",
    "redux-persist": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-get-random-values": "^1.8.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "icon-set-creator": "^1.2.6",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-native-flipper": "^0.212.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "rn-mmkv-storage-flipper": "^1.1.1",
    "run-script-os": "^1.1.6",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
